{
    "name": "discord-bot",
    "version": "0.1.0",
    "main": "src/start.ts",
    "packageManager": "pnpm@7.9.5",
    "scripts": {
        "start": "node --loader ts-node/esm/transpile-only src/start.ts",
        "dev": "nodemon --experimental-specifier-resolution=node --exec node --no-warnings=ExperimentalWarning --loader ts-node/esm/transpile-only src/start.ts",
        "build": "tsc",
        "serve": "pnpm build && node build/start.js"
    },
    "devDependencies": {
        "@types/got": "^9.6.12",
        "@types/node": "18.7.13",
        "nodemon": "2.0.19",
        "ts-node": "10.9.1",
        "typescript": "4.7.4"
    },
    "dependencies": {
        "@discordx/importer": "^1.1.10",
        "@vitalets/google-translate-api": "^8.0.0",
        "discord.js": "^14.3.0",
        "discordx": "^11.1.9",
        "dotenv": "^16.0.1",
        "reflect-metadata": "^0.1.13"
    }
}
